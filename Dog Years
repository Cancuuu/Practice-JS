
//My Age
const myAge = 17;

//
let earlyYears = 2;
earlyYears *= 10.5;

//
let laterYears = myAge - 2;

//
laterYears *= 4;  

//
myAgeInDogYears = earlyYears + laterYears

//My name
myName = "cancu".toLowerCase();

//
console.log(`My name is ${myName}. I am ${myAgeInDogYears} years old in dog years.`);


